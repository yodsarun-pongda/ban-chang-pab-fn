{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "jsx": "react-jsx",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true
  },
  "include": [
    "./src/**/*"
  ],
  "allowSyntheticDefaultImports": true,
  "esModuleInterop": true,
  "strictPropertyInitialization": false,
  "noImplicitAny": false
}
